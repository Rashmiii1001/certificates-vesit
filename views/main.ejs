  <body>
<!-- Sign in page -->
<div class="loginPage">
  <nav class="navbar  justify-content-between">
    <img src="./public/images/logo.png" alt="" width="50px" height="60px">
    <h2 class="vesname">COUNCIL CERTIFICATES UPLOAD</h2>
  </nav>

  <div class="card">
    <div class="container-fluid">
      <br><br>
      <h1>Log In</h1>
      <h4>Use your VES Account to Log In</h4>
      <br><br>
      <form class="form">
        <div class="form-group">
            <input style="width:62.5%; margin-left:20%;" class="form-control" type="email" id="username" placeholder="society.vesit@ves.ac.in" value=".vesit@ves.ac.in" required="required">
        </div>
        <div class="form-group">
            <input style="width:62.5%; margin-left:20%;" type="password" id="password" class="form-control" placeholder="Password" required="required">
        </div>

        <button class="but btn btn-warning btn-lg" type="button">Log In</button>
      </form>
    </div>
  </div>
</div>
</body>
